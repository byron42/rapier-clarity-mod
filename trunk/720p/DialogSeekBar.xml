<!--******** HACKED!!! ************************-->
		<control type="group">
			<!--normal zoom for video/no music cd-->
			<animation effect="zoom" start="300,715r,1280,720" end="300,715r,1080,720" time="150" condition="[Player.HasVideo + !Window.IsActive(2901) + [Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding]] | [[Player.HasAudio + !Skin.HasSetting(HidecdArt)] + [Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding]]">VisibleChange</animation>
			<!--zoom for no music cd specific-->
			<animation effect="zoom" start="285,715r,1280,720" end="285,715r,1095,720" time="150" condition="Player.HasAudio + [Skin.HasSetting(HidecdArt) + [Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding]]">VisibleChange</animation>
			<!--empty thumb/no cd-->
			<animation effect="slide" end="-10,0" condition="Player.HasAudio + Skin.HasSetting(HidecdArt)">Conditional</animation>
			<animation effect="slide" end="20,0" condition="Player.HasAudio + !Skin.HasSetting(HidecdArt)">Conditional</animation>
			<!-- visualization with musicOSD -->
			<animation effect="zoom" start="0,0,1280,720" end="0,0,1510,720" time="150" condition="[Window.IsActive(2006) + Player.ShowInfo + Window.IsActive(120)] | Window.IsActive(2901)">Conditional</animation>
			<animation effect="slide" end="0,255" time="250" tween="circle" easing="out" condition="Window.IsActive(120) | Window.IsActive(2901)">Conditional</animation>
			<control type="slider" id="401">
				<description>Seek Slider</description>
				<posx>20</posx>
