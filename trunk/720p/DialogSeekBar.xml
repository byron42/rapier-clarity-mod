<window id="115">
	<zorder>1000</zorder>
	<defaultcontrol>1</defaultcontrol>
	<visible>Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding</visible>
	<!--DialogOpenCloseAnim-->
	<animation condition="![Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo] | Window.IsVisible(114)]" type="WindowOpen">
		<effect type="fade" end="100" time="150"/>
		<!-- <effect type="slide" start="0,-45" time="200" tween="circle" easing="out"/> -->
	</animation>	
	<animation condition="![Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo] | Window.IsVisible(114)]" type="WindowClose">
		<effect type="fade" end="0" time="150" />
		<!-- <effect type="slide" end="0,45" time="200" tween="circle" easing="out"/> -->
	</animation>

<!--*************** for FullScreenInfo's (and player controls!) update ********************************-->
	<animation effect="fade" end="100" time="0"  condition="[Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo] | Window.IsVisible(114)] + Player.Seeking + [!Player.Paused | Player.Paused]">WindowOpen</animation>
	<animation effect="fade"  end="0"  easing="out" condition="[Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo] | Window.IsVisible(114)] + Player.Seeking + [!Player.Paused | Player.Paused]">WindowClose</animation>
<!--***************************************************************************************************-->
	
	<coordinates>
		<system>1</system>
		<posx>278</posx>
		<posy>65</posy>
	</coordinates>
	
	<controls>
		<control type="group" id="1111">
