				<ondown>1</ondown>
				<onleft>1</onleft>
				<onright>1</onright>
				<!-- <animation effect="fade" end="15" condition="Window.IsVisible(videoosd)">Conditional</animation> -->
				<animation effect="fade" end="0" condition="Window.IsActive(Shutdownmenu)">Conditional</animation>
			</control>
