				<visible>!Player.Caching</visible>
			</control>
			<control type="progress">
				<description>Player Caching Progress Bar</description>
				<posx>229</posx>
				<posy>27</posy>
				<width>275</width>
				<height>8</height>
				<reveal>true</reveal>
				<midtexture border="2">scrollbar-slider-horz-focus.png</midtexture>
				<info>Player.CacheLevel</info>
				<visible>Player.Caching</visible>
			</control>
			<control type="slider" id="401">
				<description>Seek Slider</description>
				<posx>229</posx>
				<posy>25</posy>
				<width>275</width>
				<height>12</height>
				<texturesliderbar>seekslider.png</texturesliderbar>
				<textureslidernib>slider_nibFO.png</textureslidernib>
				<textureslidernibfocus>slider_nibFO.png</textureslidernibfocus>
				<visible>true</visible>
				<onup>1</onup>
				<ondown>1</ondown>
				<onleft>1</onleft>
				<onright>1</onright>
				<visible>![Window.IsVisible(Home) | Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo +!Player.ShowCodec] | Window.IsVisible(114) | Control.IsVisible(6002)]</visible>
			</control>
		</control>
<!--******** HACKED!!! ************************-->
		<control type="group">
			<!--normal zoom for video/no music cd-->
			<animation effect="zoom" start="300,715r,1280,720" end="300,715r,1080,720" time="150" condition="[Player.HasVideo + !Window.IsActive(2901) + [Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding]] | [[Player.HasAudio + !Skin.HasSetting(HidecdArt)] + [Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding]]">VisibleChange</animation>
			<!--zoom for no music cd specific-->
			<animation effect="zoom" start="285,715r,1280,720" end="285,715r,1095,720" time="150" condition="Player.HasAudio + [Skin.HasSetting(HidecdArt) + [Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding]]">VisibleChange</animation>
			<!--empty thumb/no cd-->
			<animation effect="slide" end="-10,0" condition="Player.HasAudio + Skin.HasSetting(HidecdArt)">Conditional</animation>
			<animation effect="slide" end="20,0" condition="Player.HasAudio + !Skin.HasSetting(HidecdArt) + ![Window.IsActive(Home) + !Window.IsActive(PlayerControls)]">Conditional</animation>
			<!-- visualization with musicOSD -->
			<animation effect="zoom" start="0,0,1280,720" end="0,0,1510,720" time="150" condition="[Window.IsActive(2006) + Player.ShowInfo + Window.IsActive(120)] | Window.IsActive(2901)">Conditional</animation>
			<animation effect="slide" end="0,255" time="250" tween="circle" easing="out" condition="Window.IsActive(120)">Conditional</animation>
			<animation effect="slide" end="0,115" time="250" tween="circle" easing="out" condition="Window.IsActive(2901)">Conditional</animation>
			<!--home now playing-->
			<animation effect="zoom" start="0,0,1280,720" end="0,0,560,720" time="0" condition="[Window.IsActive(Home) + !Window.IsActive(PlayerControls) + !Skin.HasSetting(HideNowPlayingMedia)]">Conditional</animation>
			<animation effect="slide" end="-115,123" time="0" condition="[Window.IsActive(Home) + Player.HasAudio + !Window.IsActive(PlayerControls) + !System.GetBool(LookAndFeel.EnableRSSFeeds) + !Skin.HasSetting(HideNowPlayingMedia) + Skin.HasSetting(NowPlayingAlignTop)]">Conditional</animation>
			<animation effect="slide" end="-105,93" time="0" condition="[Window.IsActive(Home) + Player.HasVideo + !Window.IsActive(PlayerControls) + !System.GetBool(LookAndFeel.EnableRSSFeeds) + !Skin.HasSetting(HideNowPlayingMedia) + Skin.HasSetting(NowPlayingAlignTop)]">Conditional</animation>
			<animation effect="slide" end="-115,157" time="0" condition="[Window.IsActive(Home) + Player.HasAudio + !Window.IsActive(PlayerControls) + System.GetBool(LookAndFeel.EnableRSSFeeds) + !Skin.HasSetting(HideNowPlayingMedia) + Skin.HasSetting(NowPlayingAlignTop)]">Conditional</animation>
			<animation effect="slide" end="-105,127" time="0" condition="[Window.IsActive(Home) + Player.HasVideo + !Window.IsActive(PlayerControls) + System.GetBool(LookAndFeel.EnableRSSFeeds) + !Skin.HasSetting(HideNowPlayingMedia) + Skin.HasSetting(NowPlayingAlignTop)]">Conditional</animation>
			<control type="slider" id="401">
				<description>Seek Slider</description>
				<posx>20</posx>
				<posy>32</posy>
				<width>800</width>
				<height>11</height>
				<texturesliderbar>seekslider.png</texturesliderbar>
				<textureslidernib>slider_nibFO.png</textureslidernib>
				<textureslidernibfocus>slider_nibFO.png</textureslidernibfocus>
				<visible>[Window.IsVisible(142) | [Window.IsVisible(2006) + Player.ShowInfo + !Player.ShowCodec] | Window.IsVisible(114) | [Window.IsActive(Home) + [!Skin.HasSetting(HideNowPlayingMedia) | Window.IsActive(PlayerControls)]]]] + Player.Seeking + [!Player.Paused | Player.Paused]</visible>
				<onup>1</onup>
				<ondown>1</ondown>
				<onleft>1</onleft>
				<onright>1</onright>
				<!-- <animation effect="fade" end="15" condition="Window.IsVisible(videoosd)">Conditional</animation> -->
				<animation effect="fade" end="0" condition="Window.IsActive(Shutdownmenu)">Conditional</animation>
				<!--bottom home now playing specific-->
				<animation effect="slide" end="-120,459" condition="Window.IsActive(Home) + !Window.IsActive(PlayerControls) + !Skin.HasSetting(NowPlayingAlignTop) + !Skin.HasSetting(HideNowPlayingMedia) + Player.HasAudio">Conditional</animation>
				<animation effect="slide" end="-100,413" condition="Window.IsActive(Home) + !Window.IsActive(PlayerControls) + !Skin.HasSetting(NowPlayingAlignTop) + !Skin.HasSetting(HideNowPlayingMedia) + Player.HasVideo">Conditional</animation>
				<!--spinning cd home specific-->
				<animation effect="slide" end="37,0" condition="Window.IsActive(Home) + !Window.IsActive(PlayerControls) + !Skin.HasSetting(HideNowPlayingMedia) + Player.HasAudio + !Skin.HasSetting(HidecdArt)">Conditional</animation>
			</control>
		</control>
<!--*******************************************-->

