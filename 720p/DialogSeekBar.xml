<window id="115">
	<zorder>1000</zorder>
	<defaultcontrol>1</defaultcontrol>
	<animation effect="fade" start="100" end="0" time="1" condition="Control.IsVisible(10)">Conditional</animation>
	<visible>Player.Seeking | Player.DisplayAfterSeek | Player.Paused | Player.Forwarding | Player.Rewinding</visible>
	<!--DialogOpenCloseAnim-->
	<animation condition="![Window.IsVisible(Home) | Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo] | Window.IsVisible(114) | Control.IsVisible(6002)]" type="WindowOpen">
		<effect type="fade" end="100" time="150"/>
		<!-- <effect type="slide" start="0,-45" time="200" tween="circle" easing="out"/> -->
	</animation>	
	<animation condition="![Window.IsVisible(Home) | Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo] | Window.IsVisible(114) | Control.IsVisible(6002)]" type="WindowClose">
		<effect type="fade" end="0" time="150" />
		<!-- <effect type="slide" end="0,45" time="200" tween="circle" easing="out"/> -->
	</animation>

<!--*************** for FullScreenInfo's (and player controls!) update ********************************-->
	<animation effect="fade" end="100" time="0"  condition="[Window.IsVisible(Home) | Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo] | Window.IsVisible(114) | Control.IsVisible(6002)] + Player.Seeking + [!Player.Paused | Player.Paused]">WindowOpen</animation>
	<animation effect="fade"  end="0"  easing="out" condition="[Window.IsVisible(Home) | Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo] | Window.IsVisible(114) | Control.IsVisible(6002)] + Player.Seeking + [!Player.Paused | Player.Paused]">WindowClose</animation>
<!--***************************************************************************************************-->
	
	<coordinates>
		<system>1</system>
		<posx>278</posx>
		<posy>65</posy>
	</coordinates>
	
	<controls>
		
		<!-- <visible>!Window.IsActive(142)</visible> -->
		<control type="group" id="1111">
			<visible>Control.IsVisible(6002) | Window.IsActive(Home) | Window.IsActive(FullScreenVideo) | Window.IsActive(Visualisation) | [Window.IsVisible(114) + Player.Seeking + [!Player.Paused | Player.Paused]]</visible>
			<animation effect="fade" start="100" end="0" delay="400" time="300" condition="Window.IsActive(Favourites)">Conditional</animation>
			<animation effect="fade" start="100" end="0" delay="200" time="150" condition="Window.IsVisible(PlayerControls) | Window.IsActive(ShutdownMenu) | Window.IsActive(selectdialog) | Window.IsActive(virtualkeyboard)">Conditional</animation>
			<!--little dialog fade out-->
			<animation effect="fade" start="100" end="30" delay="0" time="200" condition="Window.IsActive(100) | Window.IsActive(contextmenu) | Window.IsActive(numericinput) | Window.IsActive(101) | Window.IsActive(2002)">Conditional</animation>
			<!---->
			<animation effect="fade" start="0" end="0" time="0" easing="out" condition="[Window.IsVisible(Home) | Window.IsVisible(142) | [Window.IsActive(2006) + Player.ShowInfo +!Player.ShowCodec] | Window.IsVisible(114) | Control.IsVisible(6002)]">Conditional</animation>
			<!--OSDOpenCloseAnim-->
			<animation effect="fade" start="0" end="100" time="250" easing="out">WindowOpen</animation>
			<animation effect="fade" start="100" end="0" time="250" easing="out">WindowClose</animation>
		
			<control type="image">
