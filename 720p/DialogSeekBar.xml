					<posy>-4</posy>
					<width>245</width>
					<height>25</height>
					<align>center</align>
					<font>font_20</font>
					<textcolor>ThemeColor</textcolor>
					<label>$VAR[PlayerControlActionLabel]</label>
					<visible>![Player.Paused + ![Player.Seeking | Player.Caching]]</visible>
				</control>
				<control type="label" id="1122">
					<description>Paused Label</description>
					<posx>239</posx>
					<posy>-4</posy>
					<width>245</width>
					<height>25</height>
					<align>center</align>
					<font>font_20</font>
					<textcolor>White</textcolor>
					<shadowcolor>ThemeColor</shadowcolor>
					<label>[UPPERCASE]$LOCALIZE[112][/UPPERCASE]</label>
					<visible>Player.Paused + ![Player.Seeking | Player.Caching]</visible>
				</control>
				
				<!--progressbar dialog-->
				<control type="label" id="22">
					<description>Elapsed Time Label</description>
					<posx>210</posx>
					<posy>0</posy>
					<font>font_23</font>
					<textcolor>White</textcolor>
					<shadowcolor>ThemeColor</shadowcolor>
					<align>right</align>
					<info>Player.Time</info>
					<width>110</width>
					<height>16</height>
					<visible>!Player.Seeking</visible>
				</control>
				<control type="label" id="22">
