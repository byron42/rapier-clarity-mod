<window id="106">
	<defaultcontrol>1000</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<animation type="WindowOpen" reversible="false">
		<effect type="zoom" start="50" end="100" center="640,360" easing="out" tween="back" time="350" />
		<effect type="fade" start="0" end="100" time="300" />
	</animation>
	<animation type="WindowClose" reversible="false">
		<effect type="zoom" start="100" end="50" center="640,360" easing="in" tween="back" time="200" />
		<effect type="fade" start="100" end="0" time="250" />
	</animation>
	<coordinates>
		<system>1</system>
		<origin x="410" y="150">Window.IsVisible(MusicPlaylistEditor)</origin>
		<origin x="408" y="180">true</origin>
	</coordinates>
	<controls>	
		<!--************************************* DialogPopUPScreenDim *************************************-->
		<control type="image">
			<visible>Window.HasFocus(106)</visible>
			<width>1300</width>
			<height>770</height>
			<posx>-415</posx>
			<posy>-185</posy>
			<texture border="10,10,10,10">background-dim.png</texture>
			<colordiffuse></colordiffuse>
			<!--BackgroundOpenCloseAnim-->
			<animation effect="fade" start="0" end="20" time="100" condition="true">Conditional</animation> 
		</control>
		<!--************************************************************************************************-->
		<control type="grouplist" id="996">
			<posx>-28</posx>
			<width>600</width>
			<control type="button" id="1000">
				<description>Button Template</description>
				<posx>6</posx>
				<width>450</width>
				<height>40</height>
				<textwidth>420</textwidth>
				<font>font_21_UB</font>
				<shadowcolor>black</shadowcolor>
				<aligny>top</aligny>
				<textoffsety>5</textoffsety> <!--SDM-->
				<texturefocus border="10,10,10,10">Glass/ShutDownButtonF.png</texturefocus>
				<texturenofocus border="10,10,10,10">Glass/ShutDownButton.png</texturenofocus>
				<colordiffuse>FFFFFFFF</colordiffuse>
				<animation effect="zoom" end="104" center="auto" time="50">focus</animation>
				<animation effect="zoom" start="104" center="auto" time="50">unfocus</animation>
			</control>
			<control type="image">
				<posx></posx>
				<description>spacer</description>
				<posy></posy>
				<width></width>
				<height>10</height>
				<texture></texture>
				<fadetime></fadetime>
			</control>
		</control>		
	</controls>	
</window>