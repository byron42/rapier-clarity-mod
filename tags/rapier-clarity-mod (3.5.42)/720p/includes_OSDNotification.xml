<includes>    <!--REPLACED OSD NOTIFICATION INCLUDE WITH CODE TO IMPROVE MEMORY!!!-->
	<include name="OSDNotification">
		<!--OSD Notification-->
		<control type="group">
			<visible>Window.IsVisible(FullscreenVideo) | [Window.IsVisible(Visualisation) + !Player.Playing]</visible>
			<posx>950</posx>
			<posy>50</posy>
			<visible>!Window.IsActive(2901)</visible>
			<!--playstate images-->
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>300</width>
				<height>50</height>
				<colordiffuse>DDFFFFFF</colordiffuse>
				<texture>black-back.png</texture>
				<bordertexture border="3">NoCover_1.png</bordertexture>
				<bordersize>4</bordersize>
			</control>
			<control type="label">
				<description>Playing Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>31302</label>
				<visible>Player.Playing + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>Paused Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>31303</label>
				<visible>[Player.Paused + !Player.Caching] + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>Cache Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[15107] $INFO[Player.CacheLevel]%</label>
				<visible>[Player.Paused + Player.Caching] + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>Seeking Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>[UPPERCASE]$LOCALIZE[773][/UPPERCASE]</label>
				<visible>Player.Seeking</visible>
			</control>
			<control type="label">
				<description>RW x2 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31305] x2</label>
				<visible>Player.Rewinding2x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>RW x4 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31305] x4</label>
				<visible>Player.Rewinding4x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>RW x8 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31305] x8</label>
				<visible>Player.Rewinding8x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>RW x16 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31305] x16</label>
				<visible>Player.Rewinding16x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>RW x32 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31305] x32</label>
				<visible>Player.Rewinding32x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>FF x2 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31304] x2</label>
				<visible>Player.Forwarding2x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>FF x4 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31304] x4</label>
				<visible>Player.Forwarding4x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>FF x8 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31304] x8</label>
				<visible>Player.Forwarding8x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>FF x16 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31304] x16</label>
				<visible>Player.Forwarding16x + !Player.Seeking</visible>
			</control>
			<control type="label">
				<description>FF x32 Label</description>
				<posx>150</posx>
				<posy>5</posy>
				<height>20</height>
				<align>center</align>
				<aligny>top</aligny>
				<font>font15</font>
				<textcolor>Grey</textcolor>
				<label>$LOCALIZE[31304] x32</label>
				<visible>Player.Forwarding32x + !Player.Seeking</visible>
			</control>
			<!--progressbar dialog-->
			<control type="label" id="22">
				<description>Elapsed Time Label</description>
				<posx>10</posx>
				<posy>4</posy>
				<width>110</width>
				<height>20</height>
				<font>font12</font>
				<textcolor>white</textcolor>
				<align>left</align>
				<aligny>top</aligny>
				<info>Player.Time</info>
				<visible>!Player.Seeking</visible>
			</control>
			<control type="label" id="22">
				<description>Seek Time Label</description>
				<posx>10</posx>
				<posy>4</posy>
				<width>110</width>
				<height>20</height>
				<font>font12</font>
				<textcolor>white</textcolor>
				<align>left</align>
				<aligny>top</aligny>
				<info>Player.SeekTime</info>
				<visible>Player.Seeking</visible>
			</control>
			<control type="label" id="0">
				<description>Duration Label</description>
				<posx>288</posx>
				<posy>4</posy>
				<width>130</width>
				<height>20</height>
				<label>$INFO[Player.Duration]</label>
				<font>font12</font>
				<textcolor>white</textcolor>
				<aligny>top</aligny>
				<align>right</align>
			</control>
			<control type="progress" id="23">
				<description>Progressbar</description>
				<posx>10</posx>
				<posy>31</posy>
				<width>280</width>
				<height>8</height>
				<info>Player.Progress</info>
				<visible>true</visible>
			</control>
			<control type="progress">
				<description>Player Caching Progress Bar</description>
				<posx>10</posx>
				<posy>31</posy>
				<width>280</width>
				<height>8</height>
				<midtexture border="4,0,4,0">ProgressBuffer.png</midtexture>
				<info>Player.CacheLevel</info>
				<visible>Player.Caching</visible>
			</control>
			<control type="slider" id="401">
				<description>Seek Slider</description>
				<posx>10</posx>
				<posy>31</posy>
				<width>280</width>
				<height>12</height>
				<texturesliderbar>seekslider.png</texturesliderbar>
				<textureslidernib>slider_nibFO.png</textureslidernib>
				<textureslidernibfocus>slider_nibFO.png</textureslidernibfocus>
				<onup>1</onup>
				<ondown>1</ondown>
				<onleft>1</onleft>
				<onright>1</onright>
				<visible>Player.Seeking</visible>
			</control>
		</control>
	</include>
</includes>